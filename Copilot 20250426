' Import th∆∞ vi·ªán
Imports ClosedXML.Excel

Sub ClearExcelData(filePath As String)
    ' M·ªü file Excel
    Dim workbook As New XLWorkbook(filePath)
    Dim worksheet As IXLWorksheet = workbook.Worksheet(1) ' Ch·ªçn Sheet ƒë·∫ßu ti√™n

    ' X√≥a d·ªØ li·ªáu trong c·ªôt A v√† B
    worksheet.Range("A:A").Clear()
    worksheet.Range("B:B").Clear()

    ' L∆∞u file Excel
    workbook.Save()
End Sub

Imports Microsoft.Office.Interop.Excel

Sub ClearExcelData(filePath As String)
    ' T·∫°o ·ª©ng d·ª•ng Excel
    Dim excelApp
    Dim workbook As Workbook = excelApp.Workbooks.Open(filePath)
    Dim worksheet As Worksheet = workbook.Sheets(1) ' Ch·ªçn Sheet ƒë·∫ßu ti√™n

    ' X√≥a d·ªØ li·ªáu trong c·ªôt A v√† B
    worksheet.Columns("A:B").ClearContents()

    ' L∆∞u v√† ƒë√≥ng file
    workbook.Save()
    workbook.Close()
    excelApp.Quit()
End Sub



