' H√†m x√≥a d·ªØ li·ªáu
Sub ClearExcelData(filePath As String, sheetName As String)
    ' T·∫°o ·ª©ng d·ª•ng Excel
    Dim excelApp As New Application
    Dim workbook As Workbook = excelApp.Workbooks.Open(filePath)
    Dim worksheet As Worksheet = Nothing

    ' L·∫•y sheet theo t√™n
    For Each sheet In workbook.Sheets
        If sheet.Name = sheetName Then
            worksheet = sheet
            Exit For
        End If
    Next

    ' N·∫øu sheet kh√¥ng t√¨m th·∫•y
    If worksheet Is Nothing Then
        Throw New Exception("Sheet kh√¥ng t·ªìn t·∫°i: " & sheetName)
    End If

    ' X√≥a d·ªØ li·ªáu trong c·ªôt A v√† B
    worksheet.Columns("A:B").ClearContents()

    ' L∆∞u v√† ƒë√≥ng file
    workbook.Save()
    workbook.Close()
    excelApp.Quit()
End Sub
```

