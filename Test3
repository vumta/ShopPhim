function main(
  workbook: ExcelScript.Workbook,
  inputData: [string, string[][]]
) {
  const sheet = workbook.getActiveWorksheet();
  const headerRow = 2; // DÃ²ng chá»©a tÃªn cá»™t
  const startRow = 3;  // DÃ²ng báº¯t Ä‘áº§u ghi dá»¯ liá»‡u

  // Láº¥y tÃªn cá»™t tá»« dÃ²ng 2
  const usedRange = sheet.getUsedRange();
  const headers = usedRange.getValues()[headerRow - 1] as string[];

  // Táº¡o object Ä‘á»ƒ map tÃªn cá»™t -> chá»‰ sá»‘ cá»™t
  const colMap: Record<string, number> = {};
  headers.forEach((name, i) => {
    if (name) colMap[String(name).trim()] = i;
  });

  // XÃ¡c Ä‘á»‹nh sá»‘ dÃ²ng cáº§n ghi (giáº£ Ä‘á»‹nh cÃ¡c máº£ng con Ä‘á»u cÃ¹ng Ä‘á»™ dÃ i)
